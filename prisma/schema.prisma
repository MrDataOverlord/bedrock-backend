generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Device {
  id        String   @id @default(cuid())
  userId    String?
  orgId     String?
  platform  String
  pushToken String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
